{
  "presets": [
    "react",
    [
      "es2015",
      {
        "modules": false
      }
    ]
  ],
  "plugins": ["transform-object-rest-spread"],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "server": {
      "plugins": [
        [
          "css-modules-transform",
          {
            "generateScopedName": "[local]_[hash:base64:5]",
            "extensions": [".css"]
          }
        ]
      ]
    }
  }
}
